package pkg

type Pair struct {
  Left, Right, p int
}

//test for proof statement with struct
//:: ExpectedOutput(error:private_entailment_error)
requires acc(x) && x.Left == 2 
ensures  acc(x) && x.Left == 2
private {
  requires acc(x) && x.Left == 2 && x.p == 3
  ensures  acc(x) && x.Left == 2 && x.p == 3
  proof Foo {
    Foo(x)
  }
}
func Foo(x *Pair) {
  x.Left = 2
  x.p = 3
} 






