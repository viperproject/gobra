package zune

func convertDaysFixedWithSomeInvariants(totalDays int, b1 bool, b2 bool) (days, year int) {

  for (b2) {

    ghost variant := days

    if b1 {
      days -= 366
    } else {
      days -= 365
    }

    assert days < variant
  }
  return
}
