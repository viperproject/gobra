package p

func t0() {
	n := 5 % 3
	assert n == 2

	d := 5 / 3
	assert d == 1
}

func t1() {
	n := (-5) % 3
	assert n == -2

	d := (-5) / 3
	assert d == -1
}

func t2() {
	n := 5 % (-3)
	assert n == 2

	d := 5 / (-3)
	assert d == -1
}

func t3() {
	n := (-5) % (-3)
	assert n == (-2)

	d := (-5) / (-3)
	assert d == 1
}