package pkg

func foo() {
	var s int = 0
	var i int = 0
l:
	invariant 0 <= i && i <= 10
	invariant s == i
	for ;i < 10; i++ {
		invariant 0 <= j && j <= 0
		invariant s == i
		for j := 0; j < 10; j++ {
			invariant 0 <= k && k <= 0
			invariant s == i
			for k := 0; k < 10; k++ {
				s += 1
				continue l
			}
		}
	}
	assert s == 10
}
