package builtin

func test1() {
	s1 := []int{1, 2}
	s2 := []int{3, 4}
	s3 := append(s1, s2...)
	assert s3[0] == 1
	assert s3[1] == 2
	assert s3[2] == 3
	assert s3[3] == 4
	assert len(s3) == 4
}

func test2() {
	s1 := []int{1, 2}
	s3 := append(s1, 3, 4)
	assert s3[0] == 1
	assert s3[1] == 2
	assert s3[2] == 3
	assert s3[3] == 4
	assert len(s3) == 4
}