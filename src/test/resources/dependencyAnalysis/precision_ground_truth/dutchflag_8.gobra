package pkg

ensures 0 <= a && a <= b && b <= len(s)
func dutchflag (s []int, mid int, b1 bool, b2 bool) (ghost a int, ghost b int) {
  i := 0
  j := 0
  k := len(s)

  invariant 0 <= i && i <= j && j <= k && k <= len(s)
  for (j < k) {
    if (b2) {
      i = i + 1
      j = j + 1
    } else if (b1) {
      k = k - 1
    } else {
      j = j + 1
    }
  }

  a := i
  b := j
}
