package pkg

func issue () {
  assert []int{4: 20, 1: 10}[2] == 0
}

func extra1() {
	s := []int{4: 20, 1: 10}
	assert len(s) == 5 && cap(s) == 5
	assert s[0] == 0 && s[1] == 10 && s[4] == 20
}
