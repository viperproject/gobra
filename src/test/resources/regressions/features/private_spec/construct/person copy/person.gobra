package person

type Person struct {
  Name string
  Age int
  address
}

type address struct {
	street  string
	city    string
	country string
}

pred (p *Person) Mem() {
  acc(&p.street) && acc(&p.city) && acc(&p.country)
}

ensures acc(&result.Name) && acc(&result.Age) && result.Mem()
construct &Person() {
  fold result.Mem()
}





