package main

import "net"

func ipLen(ip net.IP) int

requires conn.Mem()
func closePacketConn(conn net.PacketConn) error {
    return conn.Close()
}

requires ae.Mem()
func addrErrorTimeout(ae * net.AddrError) bool {
    b := !ae.Timeout()
    assert b
    return ae.Timeout()
}

requires ae.Mem()
func addrErrorTimeoutInterface(ae net.Error) bool {
    return ae.Timeout()
}
