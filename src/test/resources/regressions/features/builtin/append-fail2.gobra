package builtin

func test1() {
	s1 := []int{1, 2}
	s2 := []int{3, 4}
	s3 := append(s1, s2...)
	assert s3[0] == 1
	assert s3[1] == 2
	assert s3[2] == 3
	assert s3[3] == 4
	assert len(s3) == 4
	//:: ExpectedOutput(assertion_error)
	assert false
}

func test2() {
	s1 := []int{1, 2}
	s3 := append(s1, 3, 4)
	assert s3[0] == 1
	assert s3[1] == 2
	assert s3[2] == 3
	//:: ExpectedOutput(assertion_error)
	assert s3[3] == 0
	assert len(s3) == 4
}

func test3() {
	s := make([]int, 1)
	s[0] = 1
	b := append(s, 2, 3, 4)
	//:: ExpectedOutput(assignment_error:permission_error)
	s[0] = 2
}

func test4() {
	s := make([]int, 1)
	s[0] = 1
	b := append(s, 2, 3, 4)
	assert s[0] == 1
	//:: ExpectedOutput(assertion_error:permission_error)
	assert b[0] == 1
}