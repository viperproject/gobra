package main

// ##(-I ./)
import bar "bar"

func client0() {
    X := &bar.Pair{Left: 2, Right: 0}

    bar.PairFunction(1,1,X)
}

func client1() {
    x := bar.GetPvt(2)

    assert x.GetPvtX() == 2

    bar.PvtFunction(x, 3)

    assert x.GetPvtX() == 5

    bar.PvtFunction(x, -2)

    assert x.GetPvtX() == 3
}