package zune


ensures days + (year - originYear) * 365 <= totalDays
func convertDaysFixedWithSomeInvariants(totalDays int, b1 bool, b2 bool) (days, year int) {

  days = totalDays
  year = originYear

  invariant days + (year - originYear) * 365 <= totalDays
  for (b2) {


    if b1 {
      days -= 366
    } else {
      days -= 365
    }
    year += 1

  }
  return
}
