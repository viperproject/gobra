package pkg


pred P()

pred P2(a int)

pred P3(a int) { a >= 0 }

type s struct { X, y int }
type S struct { X, y int }

pred P4(st *S) 
pred P5(st *S) { acc(st) && st.X >= 0 }
pred P6(st *S) { acc(st) && st.y >= 0 }

pred P7(st *s) 
pred P8(st *s) { acc(st) && st.X >= 0 }
pred P9(st *s) { acc(st) && st.y >= 0 }

pred (g *S) P10(st *s) { acc(g) && acc(st) && st.X >= 0 }

func main() {
    assert !pvt(P)
    assert !pvt(P2)
    assert !pvt(P3)
    assert !pvt(P4)
    assert !pvt(P5)
    assert pvt(P6)

    //can change if arguments are checked
    assert pvt(P7)
    assert pvt(P8)
    assert pvt(P9)

    g := &S{}
    assert pvt(g.P10)
}