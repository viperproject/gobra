package main

// ##(-I ./)
import bar "bar"

func main() {
    X := &bar.Pair{Left: 2, Right: 0}

    bar.PairFunction(1,1,X)

    x := bar.GetPvt(2)

    assert x.GetPvtX() == 2

    bar.PvtFunction(x, 3)

    assert x.GetPvtX() == 5
}