package pkg

type X adt {
  C{ c any }
}

func foo() {
  x := C{5}
  var i any = x

  assert typeOf(i) == type[X]
  assert i.(X).c == 5
  assert i == C{5}

  i = C{5}

  assert typeOf(i) == type[X]
  assert i.(X).c == 5
  assert i == C{5}
}